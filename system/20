cat /etc/passwd | awk -F: '{print $1}'

#-F - fs option defines the input field separator to be the regular expression
#
#ex:
#root
#daemon
#bin
#sys
#sync
#games
#man
#lp
#mail
#news
#uucp
#proxy
#www-data
#backup
